{
  "name": "merchant-api",
  "version": "0.0.1",
  "description": "NodeJS based high frequency payment API",
  "author": "Aler Denisov <aler.zampillo@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && ttsc -p tsconfig.build.json && cp package.json dist",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -c ttypescript -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "node dist/index.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "webpack:watch": "webpack --config webpack.config.js",
    "webpack:start": "node dist/server"
  },
  "dependencies": {
    "@nestjs/common": "^5.4.0",
    "@nestjs/core": "^5.4.0",
    "@nestjs/microservices": "^5.6.2",
    "@nestjs/swagger": "^2.5.1",
    "@nestjs/typeorm": "^5.2.2",
    "bignumber.js": "^8.0.2",
    "bn.js": "^4.11.8",
    "boom": "^7.3.0",
    "class-validator": "^0.9.1",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "ethers": "https://github.com/ethers-io/ethers.js#2ad110548dfa56a02ae8456f31a4a8ac2d0b0736",
    "jayson": "^2.1.1",
    "joi": "^14.3.1",
    "mysql2": "^1.6.4",
    "query-string": "^6.2.0",
    "redis": "^2.8.0",
    "reflect-metadata": "^0.1.12",
    "request": "^2.88.0",
    "rimraf": "^2.6.2",
    "rxjs": "^6.2.2",
    "shortid": "^2.2.14",
    "ssl-root-cas": "^1.3.1",
    "typeorm": "^0.2.12",
    "typescript": "^3.0.1"
  },
  "devDependencies": {
    "@nestjs/testing": "^5.1.0",
    "@types/bignumber.js": "^5.0.0",
    "@types/bn.js": "^4.11.4",
    "@types/boom": "^7.2.1",
    "@types/cors": "^2.8.4",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.1",
    "@types/joi": "^14.3.0",
    "@types/node": "^10.7.1",
    "@types/query-string": "^6.2.0",
    "@types/request": "^2.48.1",
    "@types/shortid": "^0.0.29",
    "@types/supertest": "^2.0.5",
    "jest": "^23.5.0",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.2",
    "supertest": "^3.1.0",
    "ts-jest": "^23.1.3",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.1",
    "ts-transformer-imports": "^0.2.0",
    "tslint": "5.11.0",
    "ttypescript": "^1.4.6",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
